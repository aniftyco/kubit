{
  "name": "@kubit/core",
  "version": "0.0.1",
  "description": "Core of AdonisJS",
  "exports": {
    ".": {
      "types": "./build/adonis-typings/index.d.ts",
      "require": "./build/providers/AppProvider.js"
    },
    "./standalone": "./build/standalone.js",
    "./config": "./build/config.js",
    "./Ignitor": "./build/src/Ignitor/index.js",
    "./commands": "./build/commands/index.js",
    "./commands/DumpRc": "./build/commands/DumpRc.js",
    "./commands/ListRoutes": "./build/commands/ListRoutes.js",
    "./commands/GenerateKey": "./build/commands/GenerateKey.js",
    "./build/standalone": "./build/standalone.js",
    "./build/config": "./build/config.js",
    "./build/src/Ignitor": "./build/src/Ignitor/index.js",
    "./build/commands": "./build/commands/index.js",
    "./build/commands/DumpRc": "./build/commands/DumpRc.js",
    "./build/commands/ListRoutes/*": "./build/commands/ListRoutes/*.js",
    "./build/commands/GenerateKey": "./build/commands/GenerateKey.js",
    "./*": "./*"
  },
  "files": [
    "build/adonis-typings",
    "build/commands",
    "build/templates",
    "build/providers",
    "build/services",
    "build/src",
    "build/instructions.js",
    "build/config.js",
    "build/config.d.ts",
    "build/standalone.js",
    "build/standalone.d.ts"
  ],
  "scripts": {
    "mrm": "mrm --preset=@kubit/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @kubit/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run clean && tsc && copyfiles templates/**/* templates/* build",
    "build": "tsc",
    "commit": "git-cz",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "sync-labels": "github-label-sync --labels ../../node_modules/@kubit/mrm-preset/gh-labels.json adonisjs/core"
  },
  "types": "./build/adonis-typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aniftyco/kubit.git"
  },
  "keywords": [
    "adonisjs",
    "framework",
    "mvc"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aniftyco/kubit/issues"
  },
  "homepage": "https://github.com/aniftyco/kubit#readme",
  "devDependencies": {
    "@kubit/assembler": "^0.0.1",
    "@kubit/fold": "^0.0.1",
    "@kubit/logger": "^0.0.1",
    "@kubit/mrm-preset": "^0.0.1",
    "@kubit/repl": "^0.0.1",
    "@kubit/require-ts": "^0.0.1",
    "@kubit/sink": "^0.0.1",
    "@japa/assert": "^1.3.6",
    "@japa/preset-adonis": "^1.2.0",
    "@japa/run-failed-tests": "^1.1.0",
    "@japa/runner": "^2.2.2",
    "@japa/spec-reporter": "^1.3.2",
    "@poppinss/dev-utils": "^2.0.3",
    "@types/node": "^18.11.9",
    "@types/supertest": "^2.0.12",
    "clear-module": "^4.1.2",
    "commitizen": "^4.2.5",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "etag": "^1.8.1",
    "github-label-sync": "^2.2.0",
    "husky": "^8.0.2",
    "mrm": "^4.1.13",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "strip-ansi": "^6.0.1",
    "supertest": "^6.3.1",
    "test-console": "^2.0.0",
    "typescript": "^4.9.3",
    "youch": "^3.2.2",
    "youch-terminal": "^2.1.5"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "node ../../node_modules/@kubit/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@kubit/ace": "^0.0.1",
    "@kubit/application": "^0.0.1",
    "@kubit/bodyparser": "^0.0.1",
    "@kubit/drive": "^0.0.1",
    "@kubit/encryption": "^0.0.1",
    "@kubit/events": "^0.0.1",
    "@kubit/hash": "^0.0.1",
    "@kubit/http-server": "^0.0.1",
    "@kubit/validator": "^0.0.1",
    "@poppinss/cliui": "^3.0.5",
    "@poppinss/manager": "^5.0.2",
    "@poppinss/utils": "^5.0.0",
    "fs-extra": "^10.1.0",
    "macroable": "^7.0.2",
    "memfs": "^3.4.12",
    "serve-static": "^1.15.0",
    "stringify-attributes": "^2.0.0"
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "templates": {
      "basePath": "./build/templates",
      "config": [
        {
          "src": "config/bodyparser.txt",
          "dest": "bodyparser"
        },
        {
          "src": "config/drive.txt",
          "dest": "drive"
        },
        {
          "src": "config/cors.txt",
          "dest": "cors"
        },
        {
          "src": "config/hash.txt",
          "dest": "hash"
        }
      ],
      "contracts": [
        {
          "src": "contracts/events.txt",
          "dest": "events"
        },
        {
          "src": "contracts/drive.txt",
          "dest": "drive"
        },
        {
          "src": "contracts/hash.txt",
          "dest": "hash"
        },
        {
          "src": "contracts/env.txt",
          "dest": "env"
        }
      ]
    },
    "commands": [
      "@kubit/core/build/commands/index.js"
    ],
    "types": "@kubit/core",
    "providers": [
      "@kubit/core"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "mrmConfig": {
    "core": true,
    "license": "MIT",
    "services": [
      "github-actions"
    ],
    "minNodeVersion": "14.15.4",
    "probotApps": [
      "stale",
      "lock"
    ],
    "runGhActionsOnWindows": true
  },
  "main": "build/index.js",
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  }
}
