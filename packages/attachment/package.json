{
  "name": "@kubit/attachment",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run copy-instructions",
    "copy-instructions": "cp src/instructions.md dist/instructions.md",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "adonis-typings",
    "templates"
  ],
  "adonisjs": {
    "instructions": "./dist/instructions.js",
    "instructionsMd": "./dist/instructions.md",
    "types": "@kubit/attachment/adonis-typings",
    "providers": [
      "@kubit/attachment"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {},
  "devDependencies": {}
}
