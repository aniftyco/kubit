{
  "name": "@kubit/queue",
  "version": "0.0.0",
  "private": true,
  "main": "./dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc -p tsconfig.json -w --preserveWatchOutput",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run copy-instructions",
    "copy-instructions": "cp src/instructions.md dist/instructions.md",
    "prepublishOnly": "npm run build"
  },
  "exports": {
    ".": "./dist/index.js",
    "./commands": "./dist/commands/index.js",
    "./commands/listener": "./dist/commands/listener.js",
    "./*": "./*"
  },
  "files": [
    "dist",
    "adonis-typings",
    "templates"
  ],
  "adonisjs": {
    "instructions": "./dist/instructions.js",
    "instructionsMd": "./dist/instructions.md",
    "types": "@kubit/queue/adonis-typings",
    "providers": [
      "@kubit/queue"
    ],
    "commands": [
      "@kubit/queue/commands"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "bullmq": "^4.12.0"
  }
}
